{
  "private": "true",
  "name": "surfscape-gateway",
  "description": "Source code of the SurfScape Web Services",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/surfscape/gateway.git"
  },
  "scripts": {
    "start": "eleventy --serve --watch",
    "html-minify": "html-minifier-terser --config-file=.html-minifier.json --input-dir=_site --output-dir=_site --file-ext=html",
    "build": "eleventy",
    "build:prod": "cross-env BUILD_TYPE=production eleventy",
    "debug": "cross-env DEBUG=Eleventy* eleventy --serve"
  },
  "keywords": [
    "eleventy",
    "services"
  ],
  "author": "The SurfScape Team",
  "license": "AGPL-3.0-only",
  "devDependencies": {
    "@11ty/eleventy": "3.0.0",
    "@11ty/eleventy-navigation": "1.0.4",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "html-minifier-terser": "^7.2.0",
    "luxon": "^3.6.1",
    "markdown-it": "14.1.0",
    "postcss": "^8.5.3",
    "postcss-import": "^16.1.0",
    "postcss-load-config": "^6.0.1",
    "cssnano": "^7.0.7"
  },
  "dependencies": {
    "eleventy-plugin-console-plus": "^0.1.1",
    "eleventy-plugin-icons": "^4.5.3",
    "lucide-static": "^0.509.0",
    "simple-icons": "^14.13.0"
  }
}
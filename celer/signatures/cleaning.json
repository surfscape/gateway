{
    "Windows": [
        {
            "name": "Temporary Files",
            "description": "Deletes the contents of the temp folders of Windows",
            "action": {
                "type": "folder-content",
                "path": "%TEMP%"
            }
        },
        {
            "name": "Image and Icon Cache",
            "description": "Cleans the thumbnail and icon cache used by Windows Explorer",
            "action": {
                "type": "content-pattern",
                "path": "%LOCALAPPDATA%\\Microsoft\\Windows\\Explorer",
                "patterns": [
                    "thumbcache_*.db",
                    "iconcache_*.db"
                ]
            },
            "requiredProcesses": [
                {
                    "name": "explorer.exe",
                    "canTerminate": true
                }
            ]
        },
        {
            "name": "Windows Update Cache",
            "description": "Cleans the cache of Windows Update downloads",
            "action": {
                "type": "folder-content",
                "path": "%WINDIR%\\SoftwareDistribution\\Download"
            }
        },
        {
            "name": "Error report and system logs",
            "description": "Cleans logs and error reports generated by Windows",
            "action": {
                "type": "folder-content",
                "path": "%ProgramData%\\Microsoft\\Windows\\WER"
            }
        },
        {
            "name": "Minidump Files",
            "description": "Removed minidump files created during system crashes",
            "action": {
                "type": "folder-content",
                "path": "%SystemRoot%\\Minidump"
            }
        },
        {
            "name": "Windows Temp Folder",
            "description": "Cleans temp files from the Windows folder",
            "action": {
                "type": "folder-content",
                "path": "%SystemRoot%\\Temp"
            }
        },
        {
            "name": "Windows Prefetch",
            "description": "Cleans prefetch files used to speed up application launch times",
            "action": {
                "type": "folder-content",
                "path": "%SystemRoot%\\Prefetch"
            }
        },
        {
            "name": "Windows Defender Quarantine",
            "description": "Delete files that were put in quarantine by Windows Defender",
            "action": {
                "type": "folder-content",
                "path": "%ProgramData%\\Microsoft\\Windows Defender\\Quarantine"
            }
        }
    ],
    "Browsers": [
        {
            "name": "Chrome Cache",
            "description": "Cleans the Google Chrome cache.",
            "action": {
                "type": "folder-content",
                "path": "%LocalAppData%\\Google\\Chrome\\User Data\\Default\\Cache"
            },
            "requiredProcesses": [
                {
                    "name": "chrome.exe",
                    "canTerminate": false
                }
            ]
        },
        {
            "name": "Edge Cache",
            "description": "Cleans the Microsoft Edge cache.",
            "action": {
                "type": "folder-content",
                "path": "%LocalAppData%\\Microsoft\\Edge\\User Data\\Default\\Cache"
            },
            "requiredProcesses": [
                {
                    "name": "msedge.exe",
                    "canTerminate": false
                }
            ]
        },
        {
            "name": "Brave Cache",
            "description": "Cleans the Brave browser cache.",
            "action": {
                "type": "folder-content",
                "path": "%LocalAppData%\\BraveSoftware\\Brave-Browser\\User Data\\Default\\Cache"
            },
            "requiredProcesses": [
                {
                    "name": "brave.exe",
                    "canTerminate": false
                }
            ]
        }
    ],
    "Office": [
        {
            "name": "Office Recent Files",
            "description": "Clears the Microsoft Office recent documents list.",
            "action": {
                "type": "folder-content",
                "path": "%AppData%\\Microsoft\\Office\\Recent"
            }
        }
    ],
    "Gaming": [
        {
            "name": "Steam Download Cache",
            "description": "Cleans temporary files from unfinished or corrupted Steam downloads.",
            "action": {
                "type": "folder-content",
                "path": "%ProgramFiles(x86)%\\Steam\\steamapps\\downloading"
            },
            "requiredProcesses": [
                {
                    "name": "steam.exe",
                    "canTerminate": false
                }
            ]
        },
        {
            "name": "Riot Client Logs",
            "description": "Cleans the Riot Client logs.",
            "action": {
                "type": "folder-content",
                "path": "%LocalAppData%\\Riot Games\\Riot Client\\Logs"
            }
        },
        {
            "name": "League of Legends Logs",
            "description": "Removes old logs for League of Legends.",
            "action": {
                "type": "folder-content",
                "path": "%LocalAppData%\\Riot Games\\League of Legends\\Logs"
            }
        },
        {
            "name": "Epic Games Launcher Logs",
            "description": "Cleans old Epic Games Launcher logs.",
            "action": {
                "type": "folder-content",
                "path": "%LocalAppData%\\EpicGamesLauncher\\Saved\\Logs"
            }
        }
    ],
    "Spotify": [
        {
            "name": "Spotify Cache",
            "description": "Cleans the Spotify cache. This does not remove downloads.",
            "action": {
                "type": "folder-content",
                "path": "%LocalAppData%\\Spotify\\Storage"
            },
            "requiredProcesses": [
                {
                    "name": "spotify.exe",
                    "canTerminate": false
                }
            ]
        }
    ]
}